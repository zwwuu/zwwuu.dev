{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "react/jsx-curly-brace-presence": [
      1,
      {
        "props": "always",
        "children": "ignore"
      }
    ],
    "react/jsx-sort-props": [
      1,
      {
        "callbacksLast": true,
        "shorthandLast": true
      }
    ]
  }
}
